{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 900
    }
  },
  "crons": [
    {
      "path": "/api/cron/daily-update",
      "schedule": "0 0 * * *"
    }
  ]
}
