{
  "hosting": {
    "site": "seershub",
    "source": ".",
    "frameworksBackend": {
      "region": "us-central1",
      "runtime": "nodejs20",
      "secrets": [
        "REGISTER_SECRET",
        "PRIVATE_KEY",
        "TREASURY_ADDRESS",
        "FOOTBALL_DATA_API_KEY"
      ]
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
        ]
      }
    ]
  }
}
